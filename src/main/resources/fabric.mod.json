{
  "schemaVersion": 1,
  "id": "servlinker",
  "version": "${version}",
  "name": "ServLinker",
  "description": "Send more inforamtion about the client to the minecraft server.",
  "authors": ["ThatOneToast"],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/servlinker/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "org.grill.servlinker.client.ServlinkerDataGenerator"
    ],
    "client": [
      "org.grill.servlinker.client.ServlinkerClient"
    ],
    "main": [
      "org.grill.servlinker.Servlinker"
    ]
  },
  "mixins": [
    "servlinker.mixins.json",
    {
      "config": "servlinker.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
